        <span class="kd">const</span> <span class="nx">frontPaddingValue</span> <span class="o">=</span> <span class="nx">prevValue</span> <span class="o">||</span> <span class="dl">''</span><span class="p">;</span> <span class="c1">//ê°’ì´ ì—†ì„ ê²½ìš° ë¹ˆ ë¬¸ìë¡œ ì´ˆê¸°í™”</span>
        <span class="kd">let</span> <span class="nx">pivotCount</span> <span class="o">=</span> <span class="nx">count</span><span class="p">;</span> <span class="c1">//ì¬ê·€ë¡œ ë“¤ì–´ì˜¬ë•Œë§ˆë‹¤ ê¸°ì¤€ Countë¥¼ ì •í•¨.</span>
        <span class="c1">//reduce ê³¼ì •ì—ì„œ countê°’ì´ ë³€ê²½ë˜ë¯€ë¡œ listì˜ ìš”ì†Œê°€ ëë‚ ë•Œ ë§ˆë‹¤ ë‹¤ì‹œ ê¸°ì¤€ Countë¥¼ ì¡ì•„ì¤Œ.</span>
        <span class="k">return</span> <span class="nx">stringList</span><span class="p">.</span><span class="nx">reduce</span><span class="p">(</span> <span class="p">(</span><span class="nx">listNumbers</span><span class="p">,</span> <span class="nx">value</span><span class="p">,</span> <span class="nx">idx</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>

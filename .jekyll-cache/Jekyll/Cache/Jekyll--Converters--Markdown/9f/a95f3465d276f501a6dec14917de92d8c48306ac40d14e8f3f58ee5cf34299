        <span class="n">newsMachine</span><span class="o">.</span><span class="na">setNewsInfo</span><span class="o">(</span><span class="s">"ì˜¤ëŠ˜ í•œíŒŒ"</span><span class="o">,</span> <span class="s">"ì „êµ­ ì˜í•˜ 18ë„ ì…ë‹ˆë‹¤."</span><span class="o">);</span> 
        <span class="n">newsMachine</span><span class="o">.</span><span class="na">setNewsInfo</span><span class="o">(</span><span class="s">"ë²›ê½ƒ ì¶•ì œí•©ë‹ˆë‹¤"</span><span class="o">,</span> <span class="s">"ë‹¤ê°™ì´ ë²šê½ƒë³´ëŸ¬~"</span><span class="o">);</span> 
    <span class="o">}</span> 
<span class="o">}</span>

      <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="nx">canRotateMap</span><span class="p">[</span><span class="nx">p</span><span class="p">][</span><span class="nx">k</span><span class="p">](</span><span class="nx">x</span><span class="p">,</span> <span class="nx">y</span><span class="p">))</span> <span class="k">continue</span><span class="p">;</span>

